<% @page_title = "Show Owner"%>
<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div class="user show">
  <h2>Show Owner</h2>

  <table summary="Owner detail view">
    <tr>
      <th>First name</th>
      <td><%= @user.first_name %></td>
    </tr>
    <tr>
      <th>Last name</th>
      <td><%= @user.last_name %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= @user.email %></td>
    </tr>
	<tr>
      <th>Squad name</th>
      <td><%= @user.password %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @user.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @user.updated_at %></td>
    </tr>
  </table>
  
</div>
